<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="root" prefHeight="900.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginController">
   <children>
      <Button fx:id="loginBtn" layoutX="548.0" layoutY="639.0" mnemonicParsing="false" onAction="#toDash" prefHeight="42.0" prefWidth="101.0" text="Login">
         <font>
            <Font size="26.0" />
         </font>
         </Button>
      <Button fx:id="signUpBtn" layoutX="513.0" layoutY="738.0" mnemonicParsing="false" onAction="#toNewUser" prefHeight="41.0" prefWidth="173.0" text="Sign Up">
         <font>
            <Font size="26.0" />
         </font>
      </Button>
      <ImageView fitHeight="444.0" fitWidth="493.0" layoutX="375.0" layoutY="44.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../Images/download.png" />
         </image>
      </ImageView>
      <TextField fx:id="userTF" layoutX="510.0" layoutY="475.0" prefHeight="37.0" prefWidth="187.0" />
      <PasswordField fx:id="pwField" layoutX="507.0" layoutY="546.0" prefHeight="36.0" prefWidth="194.0" />
      <Label layoutX="554.0" layoutY="438.0" text="Username">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label layoutX="557.0" layoutY="519.0" text="Password">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
   </children>
</AnchorPane>

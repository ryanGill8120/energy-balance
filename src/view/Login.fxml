<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="root" prefHeight="900.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginController">
   <children>
      <Button fx:id="loginBtn" layoutX="246.0" layoutY="552.0" mnemonicParsing="false" onAction="#toDash" prefHeight="68.0" prefWidth="101.0" text="Login">
         <font>
            <Font size="26.0" />
         </font>
         </Button>
      <Button fx:id="signUpBtn" layoutX="514.0" layoutY="557.0" mnemonicParsing="false" onAction="#toNewUser" prefHeight="58.0" prefWidth="173.0" text="Sign Up">
         <font>
            <Font size="26.0" />
         </font>
      </Button>
      <ImageView fitHeight="444.0" fitWidth="493.0" layoutX="246.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../Images/download.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>

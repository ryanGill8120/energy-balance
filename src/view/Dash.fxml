<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="dash" prefHeight="900.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.DashController">
   <children>
      <Button fx:id="logoutBtn" layoutX="315.0" layoutY="59.0" mnemonicParsing="false" onAction="#toLogin" prefHeight="56.0" prefWidth="117.0" text="Log Out">
         <font>
            <Font size="22.0" />
         </font></Button>
      <Button fx:id="addMealBtn" layoutX="85.0" layoutY="56.0" mnemonicParsing="false" onAction="#toMeal" prefHeight="67.0" prefWidth="166.0" text="Manage Days">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button fx:id="newFoodBtn" layoutX="492.0" layoutY="64.0" mnemonicParsing="false" onAction="#openNewFood" prefHeight="48.0" prefWidth="140.0" text="Add New Food">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="newWorkoutBtn" layoutX="724.0" layoutY="66.0" mnemonicParsing="false" onAction="#openNewWorkout" text="Add New Workout">
         <font>
            <Font size="26.0" />
         </font>
      </Button>
      <LineChart layoutX="218.0" layoutY="215.0" prefHeight="481.0" prefWidth="691.0">
        <xAxis>
          <CategoryAxis side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis side="LEFT" />
        </yAxis>
      </LineChart>
   </children>
</AnchorPane>

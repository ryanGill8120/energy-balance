<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="foodInput" prefHeight="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.FoodInputController">
   <children>
      <Label layoutX="294.0" layoutY="66.0" prefHeight="76.0" prefWidth="211.0" text="Food Input Screen">
         <font>
            <Font size="26.0" />
         </font>
      </Label>
      <Button fx:id="submitBtn" layoutX="352.0" layoutY="625.0" mnemonicParsing="false" onAction="#process" text="Submit">
         <font>
            <Font size="21.0" />
         </font>
      </Button>
      <Label layoutX="110.0" layoutY="209.0" text="Food Name">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Label layoutX="68.0" layoutY="306.0" text="Calories per Serving">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <TextField fx:id="nameTF" layoutX="81.0" layoutY="255.0" />
      <TextField fx:id="calTF" layoutX="87.0" layoutY="367.0" />
      <Label layoutX="110.0" layoutY="436.0" text="Serving Size">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <TextField fx:id="servingSizeTF" layoutX="86.0" layoutY="502.0" />
      <Button layoutX="486.0" layoutY="169.0" mnemonicParsing="false" onAction="#chooseFoodPic" text="Choose Picture" />
      <ImageView fx:id="foodImg" fitHeight="254.0" fitWidth="311.0" layoutX="381.0" layoutY="230.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../staticFiles/NoFood.png" />
         </image></ImageView>
   </children>
</AnchorPane>

      <Label fx:id="calBurnedAmount" layoutX="845.0" layoutY="442.0" text="Label" />
      <Label fx:id="repNumberLbl" layoutX="1003.0" layoutY="439.0" text="Label" />
      <Label fx:id="calPerServingLbl" layoutX="155.0" layoutY="254.0" prefHeight="30.0" prefWidth="124.0" text="Calories per Serving:">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label layoutX="666.0" layoutY="581.0" text="Basal Metabolism" />
      <Button fx:id="basalBtn" layoutX="780.0" layoutY="575.0" mnemonicParsing="false" onAction="#getBaslAlert" text="?" />
      <Label layoutX="658.0" layoutY="649.0" text="Total Calories Burned" />
      <Label fx:id="totalCalConsumed" layoutX="509.0" layoutY="695.0" text="Label" />
      <Label fx:id="basalLbl" layoutX="706.0" layoutY="617.0" text="Label" />
      <Label fx:id="totalCalBurned" layoutX="706.0" layoutY="695.0" text="Label" />
      <Label fx:id="calPerServingAmount" layoutX="206.0" layoutY="288.0" text="Label" />
      <Label fx:id="calBurnedPerServingLbl" layoutX="938.0" layoutY="261.0" text="Calories Burned per rep" />
      <Label fx:id="calBurnedPerRepAmount" layoutX="988.0" layoutY="311.0" text="Label" />
      <Button layoutX="22.0" layoutY="161.0" mnemonicParsing="false" onAction="#refreshFood" text="Refresh Food">
         <effect>
            <Glow />
         </effect></Button>
      <Button layoutX="1073.0" layoutY="148.0" mnemonicParsing="false" onAction="#refreshWorkout" text="Refresh Workout" />
   </children>
</AnchorPane>
